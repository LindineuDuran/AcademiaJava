<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>ServletOlaMundo</servlet-name>
		<servlet-class>servlets.OlaMundo</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ServletLerDados</servlet-name>
		<display-name>LerDados</display-name>
		<description></description>
		<servlet-class>servlets.LerDados</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServletContador</servlet-name>
		<display-name>Contador</display-name>
		<description></description>
		<servlet-class>servlets.Contador</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ServletOlaMundo</servlet-name>
		<url-pattern>/olamundo</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletLerDados</servlet-name>
		<url-pattern>/lerdados</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ServletContador</servlet-name>
		<url-pattern>/contador</url-pattern>
	</servlet-mapping>
</web-app>
